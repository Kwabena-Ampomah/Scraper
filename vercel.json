{
  "version": 2,
  "functions": {
    "backend/api/health.js": {
      "runtime": "nodejs18.x"
    },
    "backend/api/insights/dashboard.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/backend/api/health.js"
    },
    {
      "source": "/api/insights/dashboard/:productId",
      "destination": "/backend/api/insights/dashboard.js"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}
